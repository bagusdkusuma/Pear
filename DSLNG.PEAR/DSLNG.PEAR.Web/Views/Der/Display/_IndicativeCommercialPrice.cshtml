@model DSLNG.PEAR.Web.ViewModels.Der.Display.DisplayKpiInformationViewModel

<table class="table table-bordered indicative">
    <thead>
    <tr>
        <td colspan="3" style="font-size:12px;padding:10px; ">
            <div style="height:23px;">
                DSLNG Indicative Commercial Price (JCC based)-USD/MMbtu
            </div>
            
        </td>
    </tr>
    </thead>
    <tbody>
    <tr>
        <td style="width: 34%">
            <div style="height:11px;">
                Feed Gas (GSA)
            </div>
        </td>
        <td>
            JOB @Html.DisplayCompleteDerValue(Model.KpiInformationViewModels.Single(x => x.Position == 0).DerItemValue.Value, string.Empty, string.Empty)
        </td>
        <td style="width: 34%;">
            MGDP @Html.DisplayCompleteDerValue(Model.KpiInformationViewModels.Single(x => x.Position == 3).DerItemValue.Value, string.Empty, string.Empty)
        </td>
    </tr>
    <tr>
        <td>
            <div style="height:11px;">
                LNG Price (SPA)
            </div>
        </td>
        <td>
            FOB @Html.DisplayCompleteDerValue(Model.KpiInformationViewModels.Single(x => x.Position == 1).DerItemValue.Value, string.Empty, string.Empty)
        </td>
        <td>
            DES @Html.DisplayCompleteDerValue(Model.KpiInformationViewModels.Single(x => x.Position == 2).DerItemValue.Value, string.Empty, string.Empty)
        </td>
    </tr>
    </tbody>
</table>